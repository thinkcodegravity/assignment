package oopFundamental;

public class Car {

	public static void main(String[] args) {
		
		
		//Has/instance variable: brand, model, year,  color, speed
		//Does/methods: start, stop, accelerate, brake
		
		string make = "Honda";
 		string model = "Civic" ;
 		string color = "Blue" ;
 		double speed = 185.08;
 		int year = 2011;
		}
		public void start()
		public void stop()
		public void accelerate()
		public void brake()

}
